{
  "name": "plusstock",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web --port 4000",
    "build:web": "expo build:web",
    "prebuild:web": "npm run clean",
    "deploy": "npm run clean && npm ci && npm run build:web",
    "clean": "rm -rf .expo node_modules/.cache .metro web-build dist",
    "clean:all": "rm -rf .expo node_modules/.cache .metro web-build dist node_modules && npm install --legacy-peer-deps",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@tanstack/react-query": "^5.90.2",
    "@types/react-native": "^0.72.8",
    "axios": "^1.12.2",
    "expo": "~54.0.10",
    "expo-status-bar": "~3.0.8",
    "lightweight-charts": "^5.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^15.13.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.1",
    "react-native-webview": "^13.16.0",
    "react-query": "^3.39.3"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
